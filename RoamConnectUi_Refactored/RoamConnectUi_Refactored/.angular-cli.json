{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "devops"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "assets/js",
        "assets/css",
        "assets/font",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/animate.css/animate.min.css",
        "assets/css/dataTables.bootstrap.css",
        "assets/css/dataTables.bootstrap.min.css",
        "assets/css/colReorder_button.css",
        "../node_modules/air-datepicker/dist/css/datepicker.min.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "assets/js/datatables.min.js",
        "assets/js/dataTables.bootstrap.min.js",
        "assets/js/dataTables.buttons.min.js",
        "assets/js/buttons.flash.min.js",
        "assets/js/pdfmake.min.js",
        "assets/js/vfs_fonts.js",
        "assets/js/buttons.html5.min.js",
        "assets/js/buttons.print.min.js",
        "assets/js/buttons.colVis.min.js",
        "../node_modules/air-datepicker/dist/js/datepicker.min.js",
        "../node_modules/air-datepicker/dist/js/i18n/datepicker.en.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}